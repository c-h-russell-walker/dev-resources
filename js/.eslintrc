{
  "env": {
    "browser": true,
    "node": true,
    "jasmine": true,
  },
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
    }
  },
  "rules": {
    "semi": 1,
    "max-len": [
      "error",
      {
        "code": 120,
        "tabWidth": 2
      }
    ],
    "indent": ["error", 2, {"SwitchCase": 1}],
    "no-console": 1,
    "no-extra-boolean-cast": "off"
  }
}
